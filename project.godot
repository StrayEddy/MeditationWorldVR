; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="MeditationWorldVR"
run/main_scene="res://SplashScreen.tscn"
boot_splash/image="res://images/logo/black_screen.png"
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"

[audio]

enable_audio_input=true

[autoload]

Network="*res://Network.gd"
Global="*res://Global.gd"

[debug]

settings/stdout/print_fps=true
settings/stdout/verbose_stdout=true

[display]

window/size/width=2880
window/size/height=1440
window/vsync/use_vsync=false
window/stretch/mode="2d"

[gdnative]

singletons=[  ]

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_fps=90

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/shading/force_vertex_shading.mobile=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
quality/subsurface_scattering/quality=0
